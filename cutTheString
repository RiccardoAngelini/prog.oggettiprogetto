public class cutTheString {
    private String latitude;
    private String longitude;
    public cutTheString(String latitude,String longitude) {
        this.latitude=latitude;
        this.longitude=longitude;

    }

    public void cutString(String input){
        if(input.contains(",")){
            String[] parts=input.split(",");

        }
        else {
            throw  new IllegalArgumentException("String " + input + " does not contain ,");
        }
    }
    public String getLatitude(String[] parts) {
        latitude=parts[0];
        return latitude;
    }

    public String getLongitude(String[] parts) {
        longitude=parts[1];
        return longitude;
    }
}
