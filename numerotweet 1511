import java.util.Objects;
public class numeriTweet {

  Appoggio appoggio=new Appoggio();
  Generictweet generictweet= appoggio.getGenerictweet();
  GetCordinates cords;
  Distance distanza=new Distance (cords);



 //il seguente metodo mediante le istanze delle classi Generictweet e Distance dopo ave settato un intero a 0 entra in un doppio ciclo:
  //il ciclo for setta una variabile i a 0 e l?aumenta ad ogni iterazione fino a raggiungere il valore della distanza


  public int findnumberofTweet (Generictweet generictweet,String input){
    int number=0;
    for(double i=0;i< distanza.findDistance(input) ;i++)
    {
      if(generictweet == null) //se l oggetto generictweet Ã¨ vuoto il contatore dei tweet resta a 0
        number=number+0;
      else
        number=number +1; //se viene trovato l oggetto il numero dei tweet viene aumentato
    }
    return number;
}

}

class Appoggio
{
    Generictweet generictweet;

  public Generictweet getGenerictweet() {
    return generictweet;
  }
}

class Generictweet {

}
